<%= form_for(:session, url: 'access') do |f| %>
	<div class="row">
		<div class="col-sm-5">
			<div class="form-group">	
				<label class="control-label text-inverse" for="usernameUp">E-mail</label>
				<div class="input-group input-group-in">
					<span class="input-group-addon text-muted">@</span><%= f.email_field :email, class: 'form-control' %>				
				</div>
			</div>
			<div class="form-group">	
				<label class="control-label text-inverse" for="usernameUp">Senha</label>
				<div class="input-group input-group-in">
					<span class="input-group-addon text-muted">*</span><%= f.password_field :password, class: 'form-control' %>				
				</div>
			</div>
			<div class="form-group form-actions">
				<%= f.submit "Entrar", class: "btn btn-primary" %>				
			</div>
		</div>
	</div>
<% end %>